{
  "name": "twitter-django-vue",
  "version": "1.0.0",
  "description": "A Twitter clone application built with Django and VueJS",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "clean": "rm -rf static-root/twitter",
    "prebuild": "npm run clean",
    "build": "webpack",
    "prebuild:watch": "npm run clean",
    "build:watch": "webpack --watch"
  },
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "dependencies": {
    "await-to-js": "2.0.1",
    "axios": "0.18.0",
    "date-fns": "1.29.0",
    "lodash": "4.17.10",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/polyfill": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/register": "7.0.0-beta.54",
    "babel-loader": "8.0.0-beta.4",
    "commitizen": "2.10.1",
    "css-loader": "1.0.0",
    "cz-conventional-changelog": "2.1.0",
    "file-loader": "1.1.11",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "vue-loader": "15.2.6",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.16.2",
    "webpack-cli": "3.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
